<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="struts2" namespace="/action" extends="struts-default">
		<action name="sumvip" class="action.SumVIP">
			<result name="success">/manager/vipview.jsp</result>
			<result name="failure">/manager/index.jsp</result>
		</action>
		<action name="sumclub" class="action.SumHealthClub">
			<result name="success">/manager/clubview.jsp</result>
			<result name="failure">/manager/index.jsp</result>
		</action>
		<action name="login" class="action.Login">
			<result name="success">/manager/index.jsp</result>
			<result name="failure">/manager/login.jsp</result>
		</action>
	</package>
	
</struts>
